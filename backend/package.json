{
  "name": "traceops-backend",
  "version": "0.1.0",
  "description": "TraceOps backend service - Incident timeline & root-cause engine",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "deploy:contract": "hardhat run scripts/deploy-contract.js --network sepolia",
    "deploy:local": "hardhat run scripts/deploy-contract.js --network hardhat"
  },
  "keywords": [
    "incident",
    "timeline",
    "root-cause",
    "cloudwatch"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ethers": "^5.7.2",
    "@types/node": "^20.10.5"
  },
  "devDependencies": {
    "hardhat": "^2.28.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
